
\ProvidesPackage{beamercolorthemeGatechBeaver}
\mode<presentation>  % Invoke presentation mode

\definecolor{Gold}{RGB}{179, 163, 105}  % Gatech Gold!
\definecolor{Navy}{RGB}{0, 48, 87}  % Gatech Navy!
\definecolor{White}{RGB}{255,255,255}
\definecolor{MediumGrey}{RGB}{200,200,200}
\definecolor{WarmGrey}{RGB}{214, 219, 212}
\definecolor{Black}{RGB}{0,0,0}
\definecolor{myred}{RGB}{178,34,34}
\definecolor{myblue}{RGB}{51,81,197}


\setbeamercolor{sidebar}{bg=WarmGrey}
\setbeamercolor{titlelike}{fg=Gold,bg=Navy}
\setbeamercolor{subtitle}{fg=White,bg=Navy}
\setbeamercolor{author}{fg=Navy}
\setbeamercolor{date}{fg=Navy}
\setbeamercolor{institute}{fg=Navy}

\setbeamercolor{frametitle}{fg=Navy,bg=WarmGrey}
\setbeamercolor{framesubtitle}{fg=Navy}

\setbeamercolor{normal text}{fg=Black}
\setbeamercolor{structure}{fg=Navy}
\setbeamercolor{alerted text}{fg=Navy}
\setbeamercolor{example text}{fg=Black}

\setbeamercolor{block title}{use=structure,fg=White,bg=structure.fg!75!Black}
\setbeamercolor{block title alerted}{use=alerted text,fg=White,bg=alerted text.fg!75!Black}
\setbeamercolor{block title example}{use=example text,fg=White,bg=example text.fg!75!Black}

\setbeamercolor{block body}{bg=MediumGrey!25!White,fg=Black}
\setbeamercolor{block body alerted}{bg=MediumGrey!25!White,fg=Black}
\setbeamercolor{block body example}{bg=MediumGrey!25!White,fg=Black}

\setbeamercolor{section in toc}{fg=Black,bg=White}

\setbeamercolor{navigation symbols}{fg=Gold}

\mode<all>